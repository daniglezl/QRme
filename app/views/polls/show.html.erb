<div class="forum-info">
  <h2>Poll: <%= @poll.name %></h2>
</div>

<h2>Results</h2>

<div class="container event-container">
<div class="row">
    
    <% if @poll.poll_answers.count == 0%>
        <h3>Event admin has not poll answers yet.</h3>
    <% else %>
    
   <div class="col-sm-12">
      <div class="panel panel-default polls-card">
        <div class="panel-body">
             <%= render "shared/poll_answers_table" %>
        </div>
      </div>
    </div>
    
    <%end%>

</div>

